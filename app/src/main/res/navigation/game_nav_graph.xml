<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/game_nav_graph"
    app:startDestination="@id/gameFragment">

    <fragment
        android:id="@+id/gameFragment"
        android:name="com.example.SurviveProject.GameFragment"
        android:label="fragment_game"
        tools:layout="@layout/fragment_game" >
        <action
            android:id="@+id/action_gameFragment_to_postgameFragment"
            app:destination="@id/postgameFragment" />
    </fragment>
    <fragment
        android:id="@+id/postgameFragment"
        android:name="com.example.SurviveProject.PostgameFragment"
        android:label="fragment_postgame"
        tools:layout="@layout/fragment_postgame" />
</navigation>